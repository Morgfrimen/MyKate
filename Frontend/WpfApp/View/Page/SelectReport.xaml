<Page x:Class="WpfApp.View.Page.SelectReport"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:WpfApp.View.Page"
      mc:Ignorable="d"
      d:DesignHeight="450" d:DesignWidth="800"
      d:Background="White"
      Title="Отчеты">
    <Page.CommandBindings>
        <CommandBinding Command="{x:Static local:SelectReport.NavigateReport3_1}"
                        Executed="CommandBinding_OnExecuted_Report3_1" />
        <CommandBinding Command="{x:Static local:SelectReport.NavigateReport3_3}"
                        Executed="CommandBinding_OnExecuted_Report3_3"/>
    </Page.CommandBindings>
    <Page.Resources>
        <Style TargetType="Button" BasedOn="{StaticResource Button_Menu_Standard}" />
    </Page.Resources>

    <WrapPanel Orientation="Vertical"
               VerticalAlignment="Center"
               HorizontalAlignment="Center">
        <Button Content="Отчет форма З-1"
                Command="{x:Static local:SelectReport.NavigateReport3_1}" />
        <Button Content="Отчет форма З-2" />
        <Button Content="Отчет форма З-3" 
                Command="{x:Static local:SelectReport.NavigateReport3_3}"/>
        <Button Content="Отчет форма З-4" />
    </WrapPanel>
</Page>